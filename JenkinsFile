pipeline{
  agent any
  stages {
    stage('Git Pull'){
    steps{
    echo 'Git Pull stage ......'
    git branch: 'main', credentialsId: 'Jagadeesh569', url: 'https://github.com/Jagadeesh569/SeleniumCucumberProjNov21.git'
    } 
    
    }
    
     stage('selenium run'){
    steps{
    echo 'selenium run  ......'
    bat label: '', script: 'mvn clean test -Dcucumber.options="src/test/resources/Features --tags @Smoke"'
    } 
    
    }
    
  }
  
  post{
  always{
  echo 'Inside always'
  }
  }

}
